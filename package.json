{
  "name": "render-api",
  "version": "1.2.1",
  "description": "TypeScript client for the Render API",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bun run build:esm && bun run build:cjs && bun run build:types",
    "build:esm": "bun build src/index.ts --outdir dist --format esm --sourcemap=external --target=node",
    "build:cjs": "bun build src/index.ts --outdir dist --format cjs --sourcemap=external --target=node --entry-naming [dir]/[name].cjs",
    "build:types": "dts-bundle-generator -o dist/index.d.ts src/index.ts && cp dist/index.d.ts dist/index.d.cts",
    "test": "bun test --watch",
    "test:run": "bun test",
    "lint": "biome lint src scripts",
    "lint:fix": "biome lint --write src scripts",
    "format": "biome format --write src scripts",
    "format:check": "biome format src scripts",
    "check": "biome check src scripts",
    "check:fix": "biome check --write src scripts",
    "typecheck": "tsc --noEmit",
    "repl": "bun scripts/repl.ts",
    "integration": "bun run scripts/integration-test.ts",
    "integration:ci": "bun run scripts/integration-test.ts --skip services",
    "prepublishOnly": "bun run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ttacon/render-api.git"
  },
  "keywords": [
    "render",
    "render.com",
    "api",
    "client",
    "typescript",
    "bun"
  ],
  "author": "Trey Tacon",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ttacon/render-api/issues"
  },
  "homepage": "https://github.com/ttacon/render-api#readme",
  "dependencies": {
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.13",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/bun": "^1.1.6",
    "dts-bundle-generator": "^9.5.1",
    "semantic-release": "^24.0.0",
    "typescript": "^5.4.5"
  }
}
